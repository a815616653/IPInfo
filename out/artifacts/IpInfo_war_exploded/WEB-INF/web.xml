<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>jeeweb</display-name>
    <servlet>
        <servlet-name>AccessLog</servlet-name>
        <servlet-class>com.itianyi.AccessLogAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccessLog</servlet-name>
        <url-pattern>/accesslog</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Alshow</servlet-name>
        <servlet-class>com.itianyi.AlshowAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Alshow</servlet-name>
        <url-pattern>/alshow</url-pattern>
    </servlet-mapping>
</web-app>